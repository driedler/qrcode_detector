[
    {
        "name": "RaspberryPi Zero2",
        "vendor": "linux-gcc",
        "toolchainFile": "${workspaceFolder}/toolchain-rpi.cmake",
        "cmakeSettings": {
            "PYTHON_EXECUTABLE": "${userHome}/rpi/rootfs/usr/bin/python3.9",
            "PYTHONLIBS_FOUND": 1,
            "PYTHON_MODULE_EXTENSION": ".cpython-39-arm-linux-gnueabihf.so",
            "PYTHON_PREFIX": "${userHome}/rpi/rootfs/usr",
            "PYTHON_LIBRARIES": "${userHome}/rpi/rootfs/usr/lib/python3.9",
            "PYTHON_INCLUDE_DIRS": "${userHome}/rpi/rootfs/usr/include/python3.9",
            "PYTHON_MODULE_PREFIX": ""
        },
        "preferredGenerator": {
            "name": "Ninja"
        },
        "environmentVariables": {
            "RASPBERRY_VERSION": "3",
            "RASPBIAN_ROOTFS" : "${userHome}/rpi/rootfs"
        }
    }
]